<% if params[:title] %>
	<%= title "#{truncate(h(params[:title]))} <i>#{pluralize(@snmps.pager.total, 'snmp')} found</i>".html_safe, "#{h(params[:title])}" %>
<% else %>
	<%= title "Search Results <i>#{pluralize(@snmps.pager.total, 'snmp')} found</i>".html_safe, 'Search Results' %>
<% end %>
	
<%= render :partial => 'snmps/snmps' %>